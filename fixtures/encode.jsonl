{
  // === PRIMITIVES ===

  // NUM - Immediate (0-11)
  0: <00>,
  1: <01>,
  10: <0a>,
  11: <0b>,

  // NUM - i8 (Size C)
  -1: <ff0c>,
  42: <2a0c>,
  127: <7f0c>,
  -128: <800c>,

  // NUM - i16 (Size D)
  1000: <e8030d>,
  -129: <7fff0d>,

  // NUM - i32 (Size E)
  70000: <701101000e>,

  // NUM - i64 (Size F)
  1311768465173141112: <12345678123456780f>,

  // DEC - Decimal (Scale modifier + NUM)
  3.14: <3a010d12>,
  1.1: <0b11>,

  // STR - Immediate length (0-11)
  "": <20>,
  "hi": <686922>,
  "hello": <68656c6c6f25>,

  // STR - 1-byte length (Size C)
  "abcdefghijklm": <6162636465666768696a6b6c6d0d2c>,

  // BIN - Binary data
  <010203>: <01020333>,

  // REF - Built-in constants
  null: <50>,
  true: <51>,
  false: <52>,

  // REF - Delete tombstone
  delete: <53>,

  // PTR - Pointer (Symbolic representation for fixture)
  *ptr5: <054c>,

  // === CONTAINERS ===

  // LST - Simple list
  [10, 20]: <140c0a83>,

  // LST - Empty list
  []: <80>,

  // LST - Nested list
  [[1, 2], [3, 4]]: <04038202018286>,

  // MAP - Simple map
  {a: 1}: <01612193>,

  // MAP - Empty map
  {}: <90>,

  // MAP - Multiple keys
  {x: 10, y: 20}: <140c79210a782197>,

  // === METADATA COMBINATIONS ===

  // LST + IDX - Indexed list
  [# 10, 20]: <140c0a020008c387>,

  // MAP + SCH - Schema map
  {#schema_xy 10, 20}: <140c0a054cb296>,

  // MAP + IDX + SCH - Fully indexed schema map
  {#indexed_schema_xy 10, 20}: <140c0a020008c3054cb29b>,

  // === CONCATENATION (CAT) ===

  // CAT<STR> - String rope
  ("hello", " world"): <20776f726c642668656c6c6f25ad>,

  // CAT<LST> - List concatenation
  ([1, 2], [3, 4]): <010202820304028208ca>,

  // CAT<MAP> - Map merge
  ({a: 1}, {b: 2}): <016121939302621939ca>,

  // === EDGE CASES ===

  // Large immediate (11)
  11: <0b>,

  // Boundary: 12 needs explicit encoding
  12: <0c0c>,

  // Negative zero
  0: <00>,

  // Empty string
  "": <20>,

  // Single char string
  "a": <6121>,

